{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "test": "jasmine-browser-runner runSpecs",
    "start": "ng serve --host localhost ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "yarn ng lint && yarn lint:scss",
    "lint:fix": "yarn ng lint --fix && yarn lint:scss --fix",
    "lint:js": "yarn eslint",
    "lint:scss": "stylelint src/**/*.scss",
    "utils:mockapi-local": "cd mock-api && yarn && node ./server.js ",
    "utils:mockapi": "docker-compose up -d mockapi ",
    "utils:openapigen": "npx @openapitools/openapi-generator-cli generate -i openapi/sample.yaml -g typescript-angular -o src/app/core/api/generated",
    "utils:cypress:open": "cd e2e-tests && yarn && yarn cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "bootstrap": "^5.1.3",
    "keycloak-angular": "^8.4.0",
    "keycloak-js": "^15.0.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.8",
    "@angular/compiler-cli": "~12.2.0",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@commitlint/prompt": "^13.2.0",
    "@openapitools/openapi-generator-cli": "^2.1.21",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-rxjs": "^3.3.7",
    "husky": "^7.0.2",
    "jasmine": "^3.10.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
